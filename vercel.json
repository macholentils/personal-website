{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/images/(.*)", "dest": "/images/$1" },
    {
      "src": "/(.*)",
      "headers": { "location": "https://www.ellamcinto.sh/$1" },
      "continue": false
    }
  ]
} 